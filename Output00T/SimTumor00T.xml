<?xml version="1.0"?>
<tumor-simulation-parameters>
<dataset-name>SimTumor00T</dataset-name>
<input-directory>/scratch/TumorSim/TumorSimInput</input-directory>
<output-directory>/scratch/TumorSim/Output00T</output-directory>
<deformation-seed>/scratch/TumorSim/seed00T.mha</deformation-seed>
<deformation-iterations>10</deformation-iterations>
<infiltration-iterations>60</infiltration-iterations>
<infiltration-body-force-iterations>2</infiltration-body-force-iterations>
<deformation-initial-pressure>2.5</deformation-initial-pressure>
<deformation-kappa>80</deformation-kappa>
<deformation-damping>0.9</deformation-damping>
<infiltration-time-step>0.5</infiltration-time-step>
<infiltration-early-time>2</infiltration-early-time>
<infiltration-body-force-coefficient>5</infiltration-body-force-coefficient>
<infiltration-body-force-damping>0.9</infiltration-body-force-damping>
<contrast-enhancement-type>uniform</contrast-enhancement-type>
<brain-young-modulus>694</brain-young-modulus>
<brain-poisson-ratio>0.4</brain-poisson-ratio>
<falx-young-modulus>2000</falx-young-modulus>
<falx-poisson-ratio>0.4</falx-poisson-ratio>
<infiltration-reaction-coefficient>0.1</infiltration-reaction-coefficient>
<white-matter-tensor-multiplier>10</white-matter-tensor-multiplier>
<gray-matter-tensor-multiplier>1</gray-matter-tensor-multiplier>
<gad-noise-stddev>8</gad-noise-stddev>
<t1-noise-stddev>5</t1-noise-stddev>
<t2-noise-stddev>20</t2-noise-stddev>
<flair-noise-stddev>50</flair-noise-stddev>
<gad-max-bias-degree>4</gad-max-bias-degree>
<t1-max-bias-degree>4</t1-max-bias-degree>
<t2-max-bias-degree>4</t2-max-bias-degree>
<flair-max-bias-degree>4</flair-max-bias-degree>
<disable-background>1</disable-background>
<deformation-solver-iterations>8</deformation-solver-iterations>
<infiltration-solver-iterations>8</infiltration-solver-iterations>
<number-of-threads>8</number-of-threads>
</tumor-simulation-parameters>
